mixin sidebar
  //- - var nav = $store.routing.navigate()
  //- - var nav = `${attributes.onClick}()` || "$store.routing.navigate('stuff')"
  //- - var navigate = () => {console.log('thing')}
  //- - function nav() { console.log('testing') }
  //- - var nav = "$store.routing.navigate('thing')"
  - var navigate = route => `$store.routing.navigate('${route}')`
  div(x-data class='flex flex-col h-full select-none')
    // Header
    div(class='bg-gray-900 flex items-center h-16 px-4')
      div(class='flex-shrink-0 flex items-center')
        img(
          class='h-8 w-auto'
          src='https://tailwindui.com/img/logos/workflow-logo-indigo-500-mark-white-text.svg'
          alt='Workflow'
        )
    // Midsection
    div(class='px-2 py-3 flex-1 overflow-hidden')
      // Quick Links
      a(@click=navigate('/'))
        div(
          class='bg-gray-900 text-gray-300 px-2 py-2 my-1 flex items-center space-x-3 rounded-lg hover:bg-gray-900 hover:text-gray-300 transition-all'
        )
          svg(
            class='h-6 w-6'
            xmlns='http://www.w3.org/2000/svg'
            fill='none'
            viewBox='0 0 24 24'
            stroke='currentColor'
          )
            path(
              d='M3 12l2-2m0 0l7-7 7 7M5 10v10a1 1 0 001 1h3m10-11l2 2m-2-2v10a1 1 0 01-1 1h-3m-6 0a1 1 0 001-1v-4a1 1 0 011-1h2a1 1 0 011 1v4a1 1 0 001 1m-6 0h6'
              stroke-linecap='round'
              stroke-linejoin='round'
              stroke-width='2'
            )
          p(class='text-sm font-medium') Overview
      a(@click=navigate('/new'))
        div(
          class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3 rounded-lg hover:bg-gray-900 hover:text-gray-300 transition-all'
        )
          svg(
            class='h-6 w-6'
            xmlns='http://www.w3.org/2000/svg'
            fill='none'
            viewBox='0 0 24 24'
            stroke='currentColor'
          )
            path(
              d='M9 13h6m-3-3v6m5 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z'
              stroke-linecap='round'
              stroke-linejoin='round'
              stroke-width='2'
            )
          p(class='text-sm font-medium') New
      a(@click=navigate('/edit'))
        div(
          class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3 rounded-lg hover:bg-gray-900 hover:text-gray-300 transition-all'
        )
          <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z" />
          </svg>
          p(class='text-sm font-medium') Editor
      a(@click=navigate('/checkout'))
        div(
          class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3 rounded-lg hover:bg-gray-900 hover:text-gray-300 transition-all'
        )
          <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 3h2l.4 2M7 13h10l4-8H5.4M7 13L5.4 5M7 13l-2.293 2.293c-.63.63-.184 1.707.707 1.707H17m0 0a2 2 0 100 4 2 2 0 000-4zm-8 2a2 2 0 11-4 0 2 2 0 014 0z" />
          </svg>
          p(class='text-sm font-medium') Checkout
      a(@click=navigate('/checkout?show=saved'))
        div(
          class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3 rounded-lg hover:bg-gray-900 hover:text-gray-300 transition-all'
        )
          <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 5a2 2 0 012-2h10a2 2 0 012 2v16l-7-3.5L5 21V5z" />
          </svg>
          p(class='flex-1 text-sm font-medium') Saved
          div(class='bg-blue-500 text-gray-200 text-sm text-center w-10 h-5 rounded-full') 12
      // Recently Updated
      div(class=' text-gray-400 px-2 py-2 mt-8 flex items-center space-x-3 rounded-lg')
        p(class='text-sm font-medium') Recently Updated
        <svg class="h-4 w-4" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
          <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zm1-12a1 1 0 10-2 0v4a1 1 0 00.293.707l2.828 2.829a1 1 0 101.415-1.415L11 9.586V6z" clip-rule="evenodd" />
        </svg>
      div(class='flex-1 pr-2')
        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-blue-500')
          p(class='text-sm font-medium') The State and Fate of Hell
        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-red-500')
          p(class='text-sm font-medium truncate') You Won't Know if You Don't Ask Him
        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-yellow-500')
          p(class='text-sm font-medium truncate') Subsitutes Aren't Enough
        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-pink-500')
          p(class='text-sm font-medium truncate') Markers of the Apostate
        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-green-500')
          p(class='text-sm font-medium truncate') The Only Refuge that Lasts for Eternity
        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-purple-500')
          p(class='text-sm font-medium truncate') The Enemy is Here Because of God
    // Settings
    div(class='group bg-gray-700 h-16 pl-3 pr-3 inline-flex items-center')
      <img class="h-10 w-10 rounded-full" src="https://images.unsplash.com/photo-1500648767791-00dcc994a43e?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=facearea&facepad=2.25&w=256&h=256&q=80" alt="">
      <div class='flex-1 pl-3 truncate'>
        div(class='ml-0.5 text-gray-300 font-medium truncate') Victor Hafichuk
        <div class="flex items-center text-sm text-gray-400">
          div
            <svg class="mr-1.5 h-4 w-4 text-red-400" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
              <path fill-rule="evenodd" d="M5.05 4.05a7 7 0 119.9 9.9L10 18.9l-4.95-4.95a7 7 0 010-9.9zM10 11a2 2 0 100-4 2 2 0 000 4z" clip-rule="evenodd" />
            </svg>
          p(class='truncate') Lethbridge, AB
