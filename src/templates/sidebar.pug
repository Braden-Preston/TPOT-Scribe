      div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3 rounded-lg hover:bg-gray-900 hover:text-gray-300 transition-all')
        <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5H6a2 2 0 00-2 2v11a2 2 0 002 2h11a2 2 0 002-2v-5m-1.414-9.414a2 2 0 112.828 2.828L11.828 15H9v-2.828l8.586-8.586z" />
        </svg>
        <p class='text-sm font-medium'>Editor</p>
      div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3 rounded-lg hover:bg-gray-900 hover:text-gray-300 transition-all')
        <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 3h2l.4 2M7 13h10l4-8H5.4M7 13L5.4 5M7 13l-2.293 2.293c-.63.63-.184 1.707.707 1.707H17m0 0a2 2 0 100 4 2 2 0 000-4zm-8 2a2 2 0 11-4 0 2 2 0 014 0z" />
        </svg>
        <p class='text-sm font-medium'>Checkout</p>
      div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3 rounded-lg hover:bg-gray-900 hover:text-gray-300 transition-all')
        <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 5a2 2 0 012-2h10a2 2 0 012 2v16l-7-3.5L5 21V5z" />
        </svg>
        <p class='flex-1 text-sm font-medium'>Saved</p>
        div(class='bg-blue-500 text-gray-200 text-sm text-center w-10 h-5 rounded-full') 12
      // Recently Updated
      div(class=' text-gray-400 px-2 py-2 mt-8 flex items-center space-x-3 rounded-lg')
        <p class='text-sm font-medium'>Recently Updated</p>
        <svg class="h-4 w-4" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor">
