        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-pink-500')
          <p class='text-sm font-medium truncate'>Markers of the Apostate</p>
        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-green-500')
          <p class='text-sm font-medium truncate'>The Only Refuge that Lasts for Eternity</p>
        div(class=' text-gray-400 px-2 py-2 my-1 flex items-center space-x-3')
          div(class='h-2 w-2 rounded-full bg-purple-500')
          <p class='text-sm font-medium truncate'>The Enemy is Here Because of God</p>
    // Settings
    div(class="group bg-gray-700 h-16 pl-4 pr-3 inline-flex items-center")
